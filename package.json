		"@nextcloud/vue": "^4.0.3",
		"dayjs": "^1.10.6",
		"sanitize.css": "^12.0.1",
		"uuid": "^3.4.0",
		"vue": "^2.6.14",
		"vue-tinymce-editor": "^1.6.2"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
